package for_loops;

import java.util.Scanner;

public class reverse {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n=scan.nextInt();int j=0;int i;
		int sum=0;
		while(n>0)
		{
			i=n%10;
			while(j<n)
			{
				sum=sum+(i*2);
			}
			n=n/10;
		}
		
	
	
	
	
	}

}
