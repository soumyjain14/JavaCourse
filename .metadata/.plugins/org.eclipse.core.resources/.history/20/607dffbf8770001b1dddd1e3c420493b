package classes_and_objects;

public class DynamicArrayUse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		DynamicArray d=new DynamicArray();
		for(int i=0;i<6;i++) {
			d.add(i);
		}
		
		
		
		d.set(1, 23);
		//d.print();
		//int f=d.size();
		//System.out.println(f);
		int g=d.get(1);
		System.out.println(g);

	}

}
