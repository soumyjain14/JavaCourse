package for_loops;
import java.util.*;
public class prime {

	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		int n=scn.nextInt();
		int i,j;
		boolean number=false;
		for(i=2;i<=n;i++)
		{
			for(j=2;j<=n/2;j++)
			{
				if(i%j==0) {
					number=true;
					break;
					}
			}
			if(number==false)
			{
				System.out.println(i);
				
			}
			
		}
    
	}
}
