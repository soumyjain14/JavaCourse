package for_loops;
import java.util.*;

public class for_loop1 {

	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		int n=scan.nextInt();
		int d=2;
		if(n==2)
		{
			System.out.println("2");
			
		}
		else {
			while(d<n)
			{
				if(n%2==0)
				{
					d++;
					continue;
				}else if(n%d==0) {
					d++;
					continue;
				}else
				{
					System.out.println(d);
					d++;
					continue;
				}
			}
		}
		
	}

}
