package for_loops;
import java.util.*;

public class for_loop1 {

	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		int n=scan.nextInt();
		int t3=0;
		int i=1;int var=0;
		int t1=0;int t2=-1;
		if(n==1||n==2)
		{
		     t3=1;
			System.out.println("1");
		}else {
		while(i<n-1)
		{
			t3=t2+t1;
			t2=t2+1;
			t1=t3;
			
			
			
			i++;
			
		}
		System.out.println(t3);
		}
		
	}

}
