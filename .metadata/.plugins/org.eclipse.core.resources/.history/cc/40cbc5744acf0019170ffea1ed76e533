package for_loops;

import java.util.Scanner;

public class reverse {
	public static int fact(int n) {
		int i=1;int ans=1;
		while(i<=n)
		{
			ans=ans*i;
			i++;
		}
		return ans;
	}

	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		int n=scan.nextInt();
		int r=scan.nextInt();
		int num=fact(n);		
		int dec1=fact(r);		
		int dec2=fact(n-r);		
		int ans=num/(dec1*dec2);
		System.out.println(ans);
		
	}

}
