package for_loops;

import java.util.Scanner;

public class reverse {

	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		int n=scan.nextInt();
		int r=scan.nextInt();
		int num=1;
		int i=1;
		while(i<=n)
		{
			num=num*i;
			i++;
		}
		int dec1=1;
		int j=1;
		while(j<=r)
		{
			dec1=dec1*j;
			j++;
		}
		int dec2=1;
		int k=1;
		while(k<=n-r)
		{
			dec2=dec2*k;
			k++;
		}
		int ans=num/(dec1*dec2);
		System.out.println(ans);
		
	}

}
