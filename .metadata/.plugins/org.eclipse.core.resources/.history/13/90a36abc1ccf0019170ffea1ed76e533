package for_loops;

import java.util.Scanner;
import java.math.*;

public class reverse {

	public static void main(String[] args) {
		 Scanner scan=new Scanner(System.in);
			int n=scan.nextInt();int i=1;int j=0;int k=0;
			if(n==0)
			{
				System.out.print("");
			}
			while(i<n)
			{
				if(i*i<=n)
				{
					k=i;
				}
				
				i++;
			}
			System.out.println(k);
			
			
		
	
	}

}
