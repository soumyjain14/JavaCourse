package for_loops;
import java.util.*;
public class prime {

	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		int n=scan.nextInt();
		int d=2;int a[];int b[];
		while(d<n)
		{
			if(n%d==0)
			{
				//System.out.println(d);
				d++;
				continue;
				
			}
			else {
				if((n%d)%2==0)
				{
					d++;
					continue;
				}
			
			
			else
			
			
			{
				System.out.println("2");
				System.out.println(d);
				d++;
				continue;
			}
		}
	}

	}
}
