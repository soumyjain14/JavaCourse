package for_loops;

import java.util.Scanner;
import java.math.*;

public class reverse {

	public static void main(String[] args) {
		 Scanner scan=new Scanner(System.in);
			int n=scan.nextInt();int i=1;int j=0;
			while(j>0)
			{
				j=n/(i*i);
				i++;
				continue;
			}
			
			System.out.println(j);
			
			
		
	
	}

}
