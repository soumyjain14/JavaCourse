package for_loops;
import java.util.*;
public class prime {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n=scan.nextInt();
		int i=1;int j;int ld;
		ld=n%10;
		if(ld!=0)
		{
			System.out.print(ld);
		}
		n=n/10;
		while(n>0)
		{
		
			j=n%10;
			n=n/10;
			System.out.print(j);
		}
	}
}
